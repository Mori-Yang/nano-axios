{
  "name": "mori-axios",
  "version": "1.0.0",
  "description": "mori axios",
  "author": "mori<https://github.com/Mori-Yang>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mori-Yang/mori-axios"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "unbuild --watch",
    "dev:browser": "concurrently \"pnpm dev\" \"pnpm test:browser\"",
    "build": "eslint . && unbuild",
    "test": "vitest",
    "test:debug": "vitest -u",
    "test:browser": "live-server . --open=test/xhr-test/test.html",
    "pub": "npm run build && npm login && npm publish"
  },
  "keywords": [],
  "devDependencies": {
    "@stylistic/eslint-plugin": "^4.2.0",
    "eslint": "^9.23.0",
    "live-server": "^1.2.2",
    "concurrently": "^9.1.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.9"
  },
  "packageManager": "pnpm@10.6.5"
}